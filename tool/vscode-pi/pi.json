{
  "scopeName": "source.pi",
  "name": "PI",
  "foldingStartMarker": "\\(",
  "foldingStopMarker": "\\)",
  "fileTypes": [
    "pi"
  ],
  "patterns": [
    {
      "name": "comment",
      "begin": "!",
      "end": "$"
    },
    {
      "name": "keyword.operator",
      "match": ";|\\."
    },
    {
      "name": "keyword.operator",
      "match": "->|\\+|<<|<-"
    },
    {
      "name": "keyword.operator",
      "match": "=>|<>"
    },
    {
      "name": "keyword.other",
      "match": "#attach:"
    },
    {
      "name": "keyword.other",
      "match": "#global:"
    }
  ]
}